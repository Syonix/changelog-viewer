{
    "name": "syonix/changelog-viewer",
    "description": "This package offers a nice display for change log files",
    "type": "library",
    "require": {
        "jenssegers/date": "dev-master",
        "cebe/markdown": "^1.2.1",
        "doctrine/collections": "^2.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Burri",
            "email": "michael.burri@syonix.ch"
        }
    ],
    "autoload": {
        "psr-4": { "Syonix\\ChangelogViewer\\": "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Syonix\\ChangelogViewer\\": "tests/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/syonix/date"
        }
    ]
}
